{
  "name": "ymb-agency-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "set NODE_ENV=development && next build",
    "deploy": "rimraf out && set NODE_ENV=production && next build && type nul > out/.nojekyll",
    "start": "next start",
    "lint": "eslint",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:install": "playwright install",
    "test:all": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "dompurify": "^3.2.6",
    "modern-normalize": "^3.0.1",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-responsive": "^10.0.1",
    "sass": "^1.92.0",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jsdom": "^25.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "typescript": "^5",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
